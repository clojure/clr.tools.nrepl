<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;netstandard2.1</TargetFrameworks>
  </PropertyGroup>

	<PropertyGroup>
		<PackageId>clojure.tools.nrepl</PackageId>
		<RootNamespace>clojure.tools</RootNamespace>
		<Title>clojure.tools.nrepl</Title>
		<Product>clojure.tools.nrepl</Product>
		<AssemblyTitle>clojure.tools.nrepl</AssemblyTitle>
		<Authors>ClojureCLR Contributors</Authors>
		<Description>A port of babashka/babashka.nrepl to ClojureCLR</Description>
		<Copyright>Copyright Â© Rich Hickey, Michiel Borkent, ClojureCLR Contributors 2023</Copyright>
		<PackageLicenseExpression>EPL-1.0</PackageLicenseExpression>
		<RepositoryUrl>https://github.com/clojure/clr.tools.namesapce</RepositoryUrl>
		<Company>ClojureCLR contributors</Company>
		<PackageTags>Clojure;ClojureCLR</PackageTags>
		<Version>0.1.0-alpha1</Version>
	</PropertyGroup>

  <ItemGroup>
    <PackageReference Include="clojure.tools.reader" Version="1.3.7" />
  </ItemGroup>

	<ItemGroup>
		<EmbeddedResource CopyToOutputDirectory="Never" Include="..\..\clojure\clojure\tools\**\*" />
	</ItemGroup>

</Project>
